{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icons/Avatar_24.png"
   },
   "description": "Play Farmville2 using this latest Extension from Zynga Inc.",
   "externally_connectable": {
      "matches": [ "https://*.farm2.zynga.com/*" ]
   },
   "icons": {
      "128": "icons/Avatar_128.png",
      "16": "icons/Avatar_16.png",
      "48": "icons/Avatar_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "38",
   "name": "Farmville2 X-Press",
   "permissions": [ "contentSettings", "activeTab", "storage", "https://*.zynga.com/*", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.45",
   "web_accessible_resources": [ "view/index.html", "*.png" ]
}
